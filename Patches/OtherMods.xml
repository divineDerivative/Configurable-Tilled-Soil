<?xml version="1.0" encoding="utf-8"?>
<Patch>
	<!--Make packed dirt use dirt bags, unless Soil Relocation is active-->
	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>Soil Relocation Framework (Continued)</li>
			<li>Soil Relocation Framework</li>
		</mods>
		<nomatch Class="PatchOperationFindMod">
			<mods>
				<li>Vanilla Furniture Expanded - Architect</li>
			</mods>
			<match Class="PatchOperationAdd">
				<xpath>Defs/TerrainDef[defName="VFEArch_PlayerPackedDirt"]</xpath>
				<value>
					<costList>
						<DirtBag>1</DirtBag>
					</costList>
				</value>
			</match>
		</nomatch>
	</Operation>
</Patch>